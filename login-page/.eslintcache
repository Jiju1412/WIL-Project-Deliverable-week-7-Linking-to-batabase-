 Login;",{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":1,"column":28,"nodeType":"42","messageId":"43","endLine":1,"endColumn":36},{"ruleId":"44","severity":1,"message":"45","line":50,"column":58,"nodeType":"46","endLine":50,"endColumn":68},{"ruleId":"44","severity":1,"message":"45","line":58,"column":58,"nodeType":"46","endLine":58,"endColumn":68},{"ruleId":"44","severity":1,"message":"45","line":66,"column":58,"nodeType":"46","endLine":66,"endColumn":68},{"ruleId":"44","severity":1,"message":"45","line":74,"column":58,"nodeType":"46","endLine":74,"endColumn":68},{"ruleId":"44","severity":1,"message":"45","line":82,"column":58,"nodeType":"46","endLine":82,"endColumn":68},{"ruleId":"44","severity":1,"message":"45","line":98,"column":55,"nodeType":"46","endLine":98,"endColumn":65},{"ruleId":"44","severity":1,"message":"45","line":110,"column":55,"nodeType":"46","endLine":110,"endColumn":65},{"ruleId":"40","severity":1,"message":"47","line":49,"column":7,"nodeType":"42","messageId":"43","endLine":49,"endColumn":23},"no-native-reassign",["48"],"no-negated-in-lhs",["49"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","react/no-direct-mutation-state","Do not mutate state directly. Use setState().","MemberExpression","'ShowLoginMessage' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]